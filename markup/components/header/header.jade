mixin header(data)
	header.header
		.header__top
			.header__left
				h1.header__title
					span.header__title-w Аляева Ксения
						span.header__note психолог
				.header__contacts
					a.header__email(href="mailto:alyaevaksenya@gmail.com") alyaevaksenya@gmail.com
					a.header__phone(href="tel:89852371112") 8-985-237-11-12
			a.header__photo(href="/")
				img(src="__static__/img/content/header-photo.jpg", alt="Аляева Ксения фото")

			.header__social.social
				ul.social__list
